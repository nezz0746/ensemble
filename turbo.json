{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "artifacts/**",
        "cache/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false
    }
  }
}

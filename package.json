{
  "name": "my-eth-starter",
  "author": "@nezz0746",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:force": "turbo run test --force",
